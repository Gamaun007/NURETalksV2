{
  "name": "nuretalks-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local",
    "start-prod": "ng serve -c localprod",
    "build": "ng build",
    "test": "ng test",
    "test-single-run": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier-format": "prettier --write src/**/*.ts src/**/*.scss",
    "prettier-check": "prettier --check src/**/*.ts src/**/*.scss",
    "pre-push": "npm run lint && npm run prettier-format && npm run test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-svg-icon": "^12.0.0",
    "firebase": "^7.0 || ^8.0",
    "lottie-web": "^5.7.9",
    "ngx-lottie": "^6.3.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-virtual-scroller": "^4.0.3",
    "rxjs": "~6.6.0",
    "tailwindcss": "^2.2.15",
    "text-encoding": "^0.7.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/text-encoding": "0.0.36",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
